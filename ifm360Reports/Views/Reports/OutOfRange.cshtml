
@{
    ViewData["Title"] = "Out Of Range Report";
    Layout = "~/Views/Shared/_MainLayout1.cshtml";
}




@{

}

<style>
    .preloader {
        width: 90px;
        position: fixed;
        top: 50%;
        left: 50%;
    }



    h2 {
        text-align: center;
        margin-bottom: 20px;
    }



  
</style>
<div class=" container-fluid" id="bdy">
    <div class="card shadow mt-4" style="">
        <div class="card-header py-3">
            <h6 id="header" class="m-0 font-weight-bold text-primary ">Out Of Range Report</h6>
        </div>
        <div class="card-body">
            <div class="row">
                <input type="hidden" id="txtHiddenId" value="0" />
                <input type="hidden" id="flgmode" value="ADD" />
                <input type="hidden" id="UserId" value="@ViewBag" />
                <input type="hidden" id="txtpagename" value="Out Of Range Report" />





                <div class="col-md-4" id="datafiled">
                    <div class="form-group">
                        <label for="fromDate" class="form-label">From Date</label> 
                        <input type="date" class="form-control" id="txtfromdate" name="Date" onchange="SearchData()" />

                    </div>
                </div>

                <div class="col-md-4" id="datafiled">
                    <div class="form-group">
                        <label for="txttoDate" class="form-label">To Date</label>
                        <input type="date" class="form-control" id="txttoDate" name="Date" onchange="SearchData()" />

                    </div>
                </div>


              
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Select Region</label>
                        <select class="form-control" id="Regionid" onchange="bindbranchtoreg()">
                            <option value="All">All</option>
                            @if (ViewBag.Region != null)
                        {
                                foreach (var item in ViewBag.Region as List<SelectListItem>)
                            {
                                    <option value="@item.Value">@item.Text</option>
                            }
                        }
                        </select>
                    </div>
                </div>


                <div class="col-md-4">
                    <div class="form-group">
                        <label for="branch" class="form-label">Select Branch</label>
                        <select class="form-control" id="Branchid" onchange="bindclienttofbranch()">
                            <option value="All">All</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Select Client</label>
                        <select class="form-control" id="Clientid" onchange="bindsite()">
                            <option value="All">All</option>
                       
                        </select>
                    </div>
                </div>



                <div class="col-md-4">
                    <div class="form-group">
                        <label for="shift">Select Site</label>
                        <select class="form-control" id="siteid" onchange="SearchData()">
                            <option value="All">All</option>
                        @if (ViewBag.Shift != null)
                        {
                            foreach (var item in ViewBag.Shift as List<SelectListItem>)
                            {
                                    <option value="@item.Value">@item.Text </option>
                            }
                        }
                        </select>
                    </div>
                </div>





                <div class="preloaderid">  <img class="preloader" style="display:none;" src="~/loading-waiting.gif" /></div>

                <div class="col-lg-12">
                    @* <button class="btn btn-primary" onclick="SearchData()">View Shortage</button> *@
                    <button class="btn btn-primary" onclick="exportexcel()">Export To Excel</button>
                    <button class="btn btn-primary" id="Genratepdf">Export To PDF</button>

                </div>


                <div class="table-responsive mt-2">
                    <div class="table table-bordered" id="prindDiv" width="100%" cellspacing="0">
                            @*    <style>
                        table, th, td {
                        border: 1px solid black;
                        border-collapse: collapse;
                        text-align:center;

                        }


                        </style> *@
                        <table id="data-table">

                            <thead>
                                <tr style="background-color: #c84143 !important;">
                                    <th>S.No</th>
                                    <th>Region</th>
                                    <th>Branch</th>
                                    <th>Client Name</th>
                                    <th>Site Name</th>
                                    <th>Employee Code</th>
                                    <th>Employee Name</th>
                                    <th>Date</th>
                                    <th>Time</th>
                                    <th>Location Address</th>
                                    <th>Latitude</th>
                                    <th>Longitude</th>
                                    <th>Distance From Site (In KM)</th>
                                </tr>
                            </thead>
                            <tbody class="companybody">
                            </tbody>
                        </table>
                    </div>

                    <div class="table table-bordered" id="prindDivHid" style="display:none;" width="100%" cellspacing="0">
                            @*    <style>
                        table, th, td {
                        border: 1px solid black;
                        border-collapse: collapse;
                        text-align:center;

                        }


                        </style> *@

                        <table id="data-tablehid">

                            <thead>

                                <tr>
                                    <th>S.No</th>
                                    <th>Region</th>
                                    <th>Branch</th>
                                    <th>Client Name</th>
                                    <th>Site Name</th>
                                    <th>Emp Code</th>
                                    <th>Emp Name</th>
                                    <th>Date</th>
                                    <th>Time</th>
                                    <th>Location Address</th>
                                    <th>Latitude</th>
                                    <th>Longitude</th>
                                    <th>Distance From Site (In KM)</th>
                                </tr>




                            </thead>

                            <tbody class="companybody">
                            </tbody>

                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>

</div>

<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="~/js/reports/Outofrange.js"></script>
<script src="~/js/Master.js"></script>
<script src="~/js/InsAjax.js"></script>
<script src="~/js/xlxs.full.min.js"></script>














