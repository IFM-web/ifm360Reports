

@{
    ViewData["Title"] = "User Map To Customer";
    Layout = "~/Views/Shared/_MainLayout1.cshtml";
}


<div class=" container-fluid" id="bdy">
    <div class="card shadow mt-4" style="">
        <div class="card-header py-3">

            <h6 id="header" class="m-0 font-weight-bold text-primary ">User Map To Customer</h6>
        </div>
        <div class="card-body">
            <div class="row">
               
                <div class="col-md-4">
                    <div class="form-group">
                        <label>User</label>
                        <select class="form-control mandatory" name="User"  id="Userselect">
                            <option value="0">Select</option>
                            @foreach (var e in ViewBag.User as List<SelectListItem>)
                            {
                                <option value="@e.Value">@e.Text</option>
                            }
                        </select>
                    </div>
                </div>
                
              

                <div class="col-md-4">
                    <div class="form-group">
                        <label>Customer</label>
                        <select class="form-control mandatory" id="Customer" name="Customer" multiple="multiple">
                          @*   <option value="All">All</option>
                            @foreach (var e in ViewBag.Customer as List<SelectListItem>)
                            {
                                <option value="@e.Value">@e.Text</option>
                            } *@
                        </select>
                    </div>
                </div> 

                <div class="col-md-2 ">
                    <div class="form-group">
                        <button class="btn btn-primary px-4" onclick="SaveData()">Save</button>
                    </div>
                </div>

            </div>
   


            <div id="printdiv"></div>


        </div>

    </div>

</div>
