@{
    ViewData["Title"] = "CheckList";
    Layout = "~/Views/Shared/_MainLayout1.cshtml";
}

<div class=" container-fluid" id="bdy">
    <div class="card shadow mt-4" style="">
        <div class="card-header py-3">
            <h6 id="header" class="m-0 pl-1 font-weight-bold text-primary ">Check List</h6>
        </div>
        <div class="card-body">
            <div class="row">

                <div class="col-md-6">
                    <div class="form-group">
                        <select id="ddlClient" onchange="show(this.value)" class="form-control">
                            <option value="0">select</option>
                            @if (ViewBag.client != null)
                            {
                                foreach (var item in ViewBag.client)
                                {
                                    <option value="@item.Value">@item.Text</option>
                                }
                            }
                        </select>
                        <label for="ddlClient">Client</label>
                    </div>
                </div>
                
                <div class="col-md-2">
                    <button class="btn btn-danger">Save</button>
                </div>

            </div>
            <div class="row">
                <div class="col-12">
                    <div id="printDiv"></div>
                </div>
            </div>
        </div>
    </div>

</div>

<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<link rel="stylesheet" href="https://cdn.datatables.net/1.13.5/css/jquery.dataTables.min.css">
<script src="https://cdn.datatables.net/1.13.5/js/jquery.dataTables.min.js"></script>

<script src="~/js/master/clientbudget.js"></script>
